package com.java.MyProject;


public class NextGreatestElement {
	public static void main(String[] args) {
		
	
	int arr[]= {6,1,3,2,4};
	int n = arr.length;
	
	NextGreatestElement(arr, n);}

	private static void NextGreatestElement(int[] arr, int n) {
	
	int next =0;
	for (int i = 0; i < n; i++) {
		next = -1;
		for (int j = i+1; j < n; j++) {
				
			
			 if (arr[i] < arr[j]) {
                 next = arr[j];
                 break; }  }
         System.out.print( next+ " , "); }}
	
	
}
