	Scanner s = new Scanner(System.in);
		System.out.println("Enter a number: ");
		int num= s.nextInt();
		int temp=0;
for(int i=2;i<=num;i++) {
	if(num%i==0 ) {
	temp++;}}

if(num==1) {
	System.out.println(num+ " is not a prime number");}	
else if(temp<=1) {
	System.out.println(num+ " is a prime number");}

	else {
		System.out.println(num+" is not a prime number");}
	
}}
