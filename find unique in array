
    public static int findUnique(int[] arr){
		
    Arrays.sort(arr);
    int ans= -1;
    for(int i=0; i< arr.length; i++){
      for(int j=i+1; j< arr.length; j++){
      if(arr[i]== arr[j]){
      arr[i]= arr[j]=0;
      }}}


 for(int i=0; i< arr.length; i++){
   if(arr[i]>0){
     ans= arr[i];}}

    return ans;
    
}}
