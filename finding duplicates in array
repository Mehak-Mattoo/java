 int arr[]= {1,2,7,5,2,8,9,2};
 int temp;

for (int i = 0; i < arr.length; i++) {
    for (int j = i+1; j < arr.length; j++) {
      
    
       if(arr[i]>arr[j]) {
        temp = arr[i];
        arr[i]= arr[j];
        arr[j]= temp;}
        
        if(arr[i]==arr[j]) {
        
       System.out.println(arr[i]);    }}
}
}}
