
	int Array[] = {1,25,9,1,7,86};
	int max = Array[0];
	for (int i = 0; i < Array.length; i++) {
		if(Array[i]>max) {
		max= Array[i];	}}
	
	int min = Array[0];
	for (int j = 0; j < Array.length; j++) {
	
	
		if(Array[j] != max && Array[j]> min ) {
			min= Array[j];
		}
		}
		System.out.println(max+" and "+ min);	
}
}
