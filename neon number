
	int num= 9;
		int square = num*num;
		int remainder =0; int  quotient=0; int sum=0;
		
		remainder+= square% 10;
		quotient= square/10;
		 
		sum= remainder+ quotient;
		System.out.println("remainder= "+remainder+"\nquoteint= "+quotient+ "\nsum = "+sum);
		
		if(sum== num) {
			System.out.println(num +" is a neon number");}
		else {
			System.out.println(num+" is not a neon number");}		
			
		}
	}
