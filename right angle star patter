import java.util.Scanner;

public class Solution {


	public static void main(String[] args) {
		
		Scanner s= new Scanner(System.in);
int n= s.nextInt();


int rowCount= 1;


for(int i= 0; i<n ; i++){
for(int j= 0; j<=i ; j++){	
System.out.print("*");
rowCount++;}

System.out.println();}

/*

*
**
***
****

*/
		
	}

}
