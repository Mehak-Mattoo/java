public class Solution {
    public static int subarraysWithSumK(int []a, int b) {

  int count=0;   
     for (int i = 0; i < a.length; i++) {
         for (int j = i; j < a.length; j++) {
             int xor=0;
             for (int j2 = i; j2 <=j; j2++) {
                 xor= xor ^a[j2];}

                 if(xor==b){
                     count++;
                 }
             
         }
     }return count;


    }
}
