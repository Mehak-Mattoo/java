 public class Solution { // idiotic 
    public static boolean uniqueBinaryTree(int a, int b){
    
      if(a==1 && b==2){
          return true;
      }
       else if(a==2 && b==1){
          return true;
      }
      else if(a==2 && b==3){
          return true;
      }
      else if(a==3 && b==2){
            return true;

        }
    return false;


    }
}
