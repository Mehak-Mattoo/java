import java.util.* ;
import java.io.*; 

public class Solution {
	public static int sqrtN(long N) {

long low=1, high= N, ans=0;

while(low<=high){
    long mid= (low+high)/2;
    long square= mid*mid;

    
    if( square<=N){ 
      low= mid+1;
     ans= mid; }

    else{
    high= mid-1;
     }
    
}return (int)ans;

    
}}
